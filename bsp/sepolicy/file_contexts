# Bluetooth
/dev/ttyAMA0                                           u:object_r:bluetoothtbd_device:s0
# Serial console
/dev/ttyS0                                             u:object_r:console_device:s0
# Emmc device
/dev/block/mmcblk0          u:object_r:mmc_block_device:s0
/dev/block/by-name/boot     u:object_r:boot_block_device:s0
/dev/block/by-name/system   u:object_r:system_block_device:s0

# CRDA files
/system/bin/crdad           u:object_r:crdad_exec:s0

# fastbootd
/system/bin/fastbootd       u:object_r:fastbootd_exec:s0
